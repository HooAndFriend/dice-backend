{
  "name": "dice-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "start": "nest start -b swc",
    "dev": "cross-env NODE_ENV=development nest start -b swc -w",
    "start:debug": "cross-env NODE_ENV=staging nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker": "docker build --no-cache --rm -f Dockerfile -t pinomaker/dice-backend:latest . --platform linux/amd64",
    "docker-push": "docker push pinomaker/dice-backend:latest",
    "docker-buildx": "docker buildx build --platform linux/arm64,linux/amd64 -t pinomaker/dice-backend:latest-arm64 -t pinomaker/dice-backend:latest-amd64 --file Dockerfile . --push",
    "compodoc": "npx @compodoc/compodoc -p tsconfig.json -s"
  },
  "dependencies": {
    "@nestjs-modules/mailer": "^1.10.3",
    "@nestjs/cache-manager": "^2.1.1",
    "@nestjs/common": "^10.2.6",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.6",
    "@nestjs/devtools-integration": "^0.1.5",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.2",
    "@nestjs/platform-express": "^10.2.6",
    "@nestjs/swagger": "^7.1.12",
    "@nestjs/typeorm": "^10.0.0",
    "@types/redis": "^4.0.11",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.3.1",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "hbs": "^4.2.0",
    "loadsh": "^0.0.4",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.1",
    "nodemailer": "^6.9.5",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "redis": "^4.6.11",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^4.6.3",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    "@nestjs/cli": "^10.1.18",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.6",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.90",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.18",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.11",
    "@types/passport": "^1.0.13",
    "@types/passport-jwt": "^3.0.10",
    "@types/supertest": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "csurf": "^1.11.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "helmet": "^7.0.0",
    "jest": "29.3.1",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testMatch": [
      "**/*.spec.ts"
    ],
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    }
  }
}
